<!-- Landing -->
<section id="home" class="home intro flx-col">
  <div class="intro-bg-white content-section">
    <div class="max-content-width">
      <div class="flx-row">
        <div class="flx-col rel">
          <h1 class="opacity-0 fade-up" data-observing="true">SURF</h1>
        </div>
        <div class="flx-col intro-text-wrapper">
    {{!-- <h2 class="opacity-0 fade-up" data-observing="true"><span class="white">{{{ getString "title"}}}</span></h2> --}}
          {{#each (landingCopy)}}
            {{#if this.callout}}
              <div class="text-callout-wrapper intro-text-callout-wrapper flx-row opacity-0 fade-up" data-observing="true">
                <div class="email-icon-wrapper rel"></div>
                <p class="body-text callout-text">{{{ this.text }}}</p>
              </div>
            {{else}}
              <p class="body-text txt-dark opacity-0 fade-up" data-observing="true">{{{ this.text }}}</p>
            {{/if}}
          {{/each}}
        </div>
      </div>
    </div>
  </div>
</section>


<!-- People -->
<section id="people" class="content-section surf-people-wrapper flx-col">
  <div class="max-content-width">
    <h3 class="ff-Met">People</h3>
  </div>
  <div class="max-content-width flx-row thirds">
    {{#each (person)}}
      <div class="bio-wrapper opacity-0 fade-up flx-third" data-observing="true">
        <div class="bio-img {{ this.imgClassName }} flx-third-img"></div>
        <div class="bio-content">
          <h4 class="ff-Met bio-name">{{ this.name }}</h4>
          <p class="bio-title ff-Met">{{ this.jobTitle }}</p>
          <p class="bio-affiliation ff-Met">{{ this.affiliation }}</p>
          <p class="bio-text body-text">{{ this.bio }}</p>
        </div>
      </div>
    {{/each}}
  </div>
</section>

<!-- Summits -->
<section id="summits" class="content-section summits flx-col">
  <div class="max-content-width">
    <h3 class="opacity-0" data-observing="true">Summits</h3>
  </div>
  <div class="max-content-width flx-row thirds">
    {{#each (summits)}}
      <div class="flx-third summit-wrapper flx-col fade-up opacity-0" data-observing="true">
        <div class="summit-image bg-img flx-third-img {{ this.imgClassName }}"></div>
        <div class="summit-info">
          <h4 class="summit-name">{{ this.summitName }}</h4>
          <p class="summit-location">{{ this.summitLocation }}</p>
          <p class="summit-date">{{ this.summitDate }}</p>
        </div>
      </div>
    {{/each}}
  </div>
</section>
<!-- Publications -->
<section id="publications" class="content-section publications flx-col">
  <div class="max-content-width">
    <div class="flx-row">
      <div class="flx-col third rel">
        <div class="docs" data-observing="true">
          <div class="doc doc-3 bg-img"></div>
          <div class="doc doc-2 bg-img"></div>
          <div class="doc doc-1 bg-img"></div>
        </div>
        <h3 class="pub-headline opacity-0" data-observing="true">Publications</h3>
      </div>
       <ul class="publications-list two-thirds opacity-0 style-none" data-observing="true">
        {{#each (publication)}}
          <li class="publication-list-item">
            {{#if this.pubLinkHref }}
              <a href="{{ this.pubLinkHref }}" target="_blank" rel="noopener noreferrer" class="pub-title">{{ this.title }}</a>
            {{else}}
              <p class="pub-title">{{ this.title }}</p>
            {{/if}}
            <p class="pub-authors">{{ this.authors }}</p>
            <p class="pub-date-location">{{ this.addInfo }}</p>
          </li>
        {{/each}}
      </ul>
      
    </div>
  </div>
</section>

