<section id="links" class="content-section links flx-col" data-observing="true" data-section-id="links">
  <div class="max-content-width">
    <div class="flx-row thirds links-thirds">
      <div class="flx-third">
        <h3 class="opacity-0  links-hl" data-observing="true">Links</h3>
      </div>
      <div class="two-thirds two-up links-two-up flx-row">
        <div class="links-third wrap-col-mobile">
          {{#uncategorizedLinks}}
            <div class="flx-col links-wrapper {{ this.listClassName }} opacity-0 fade-up" data-observing="true">
              {{#each this.linkList}}
                <a class="link pseudos dark-link" href="{{ this.linkHref }}">{{ this.linkTitle }}</a>
              {{/each}}
            </div>
          {{/uncategorizedLinks}}
          <div class="rss-feeds-wrapper links-wrapper flx-col opacity-0 fade-up" data-observing="true">
            <div class="a-d-links flx-col">
              {{#each (attackDefenseLinksAndFeed)}}
                <a class="link pseudos dark-link" href="{{ this.linkHref }}">{{ this.linkTitle }}</a>
              {{/each}}
            </div>
            <h4 class="feed-wrapper-title">
              <svg xmlns="http://www.w3.org/2000/svg" class="rss-icon" viewBox="0 0 8 8" width="256" height="256">
                <rect   stroke="none" fill="black" class="button" width="8" height="8" rx="1.5" />
                <circle fill="white" stroke="none" class="symbol" cx="2" cy="6" r="1" />
                <path  fill="white" stroke="none" class="symbol" d="m 1,4 a 3,3 0 0 1 3,3 h 1 a 4,4 0 0 0 -4,-4 z" />
                <path  fill="white" stroke="none" class="symbol" d="m 1,2 a 5,5 0 0 1 5,5 h 1 a 6,6 0 0 0 -6,-6 z" />
              </svg>
              Latest from Attack & Defense
            </h4>
            <div id="rss-feeds" class="rss-feeds feed-wrapper"></div>
          </div>
        </div>

